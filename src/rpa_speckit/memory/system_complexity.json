  "regras_aplicacao": {
    "como_usar": [
      "1. Identificar o sistema no DDP",
      "2. Verificar se está na base de dados (sistemas conhecidos)",
      "3. Se não estiver, classificar por categoria (portal governo, legado, menos conhecido)",
      "4. Aplicar multiplicador base da categoria",
      "5. Aplicar multiplicadores adicionais por fatores técnicos (interface, documentação, seletores)",
      "6. Multiplicar a estimativa base pelo multiplicador final",
      "7. Documentar na justificativa da estimativa"
    ],
    "exemplo": {
      "sistema": "e-CAC",
      "estimativa_base": "2 horas",
      "multiplicadores": {
        "sistema": 1.8,
        "interface": 1.3,
        "seletores": 1.4
      },
      "calculo": "2h × 1.8 × 1.3 × 1.4 = 6.55h ≈ 7 horas",
      "justificativa": "Portal do governo (1.8x) + Interface legada (1.3x) + Seletores instáveis (1.4x)"
    }
  }
